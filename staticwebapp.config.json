{
    "trailingSlash": "auto",
    "routes": [
        {
            "route": "/images/*",
            "headers": {
                "cache-control": "max-age=15770000"
            }
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/404.html"
        }
    },
    "globalHeaders": {
        
        
        "content-security-policy": "default-src https:; style-src https://wittallen.net 'unsafe-inline'; script-src https://wittallen.net https://plausible.io; img-src https://wittallen.net data:; font-src https://wittallen.net https://fonts.googleapis.com https://fonts.gstatic.com data:;",
        "permissions-policy": "microphone=(), camera=(), geolocation=(), display-capture=()",
        
        "Cross-Origin-Embedder-Policy-Report-Only": "require-corp",
        "Cross-Origin-Opener-Policy-Report-Only": "same-origin",
        "Cross-Origin-Resource-Policy-Report-Only": "cross-origin",
        
    },
    "mimeTypes": {
        ".json": "text/json"
    }
}